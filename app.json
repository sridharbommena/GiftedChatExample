{
  "expo": {
    "name": "Let'sChat",
    "slug": "giftedChat",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "splash": {
      "image": "./assets/Letschat.png",
      "resizeMode": "contain",
      "backgroundColor": "#264653"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "android": {
      "package": "com.sridhar20.giftedChat"
    }
  }
}
